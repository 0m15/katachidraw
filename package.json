{
  "name": "katachidraw",
  "description": "SVG based drawing tool and react-native component",
  "version": "0.0.0",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/katachidraw.git"
  },
  "source": "./src/library.ts",
  "main": "./dist/library.js",
  "types": "./dist/library.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "eslint --ext .js,.ts,.tsx src && tsc && react-scripts test --transformIgnorePatterns \"node_modules/(?!points-on-curve)/\"",
    "compile": "tsc --noEmit false --declaration --outDir dist"
  },
  "keywords": [
    "react",
    "native",
    "svg",
    "web",
    "draw"
  ],
  "license": "MIT",
  "dependencies": {
    "jotai": "^0.12.5",
    "points-on-curve": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native-svg": "^12.1.0",
    "react-native-web": "^0.14.10",
    "scheduler": "^0.20.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.63.45",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended",
      "prettier",
      "prettier/react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
